{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/app.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "internalConsoleOptions": "openOnSessionStart",
            "name": "Mocha Tests",
            "program": "test",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "runtimeExecutable": "/usr/local/bin/npm",
        }
        
    ]
}